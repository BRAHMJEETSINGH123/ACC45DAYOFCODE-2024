#include <iostream>
using namespace std;

int main() {
    int T;
    cin >> T; // number of test cases
    
    while (T--) {
        int N, X, P;
        cin >> N >> X >> P;
        
        // Calculate Chef's score
        int score = 4 * X - N;
        
        // Check if Chef passes or fails
        if (score >= P) {
            cout << "PASS" << endl;
        } else {
            cout << "FAIL" << endl;
        }
    }
    
    return 0;
}
