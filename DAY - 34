#include <iostream>
using namespace std;

int main() {
    int T;
    cin >> T;  // Number of test cases
    
    while (T--) {
        int A, B;
        cin >> A >> B;  // First and second drawn numbers
        
        int C = 21 - (A + B);  // Calculate the required third number
        
        // Check if C is between 1 and 10
        if (C >= 1 && C <= 10) {
            cout << C << endl;  // If valid, output C
        } else {
            cout << -1 << endl;  // If not valid, output -1
        }
    }
    
    return 0;
}
