# Read the number of test cases
T = int(input())

# Loop over each test case
for _ in range(T):
    # Read X and Y
    X, Y = map(int, input().split())
    
    # Determine which option is faster or if they take the same time
    if X < Y:
        print("BIKE")
    elif X > Y:
        print("CAR")
    else:
        print("SAME")
