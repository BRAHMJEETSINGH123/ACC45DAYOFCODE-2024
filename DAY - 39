#include <iostream>
using namespace std;

int main() {
    int T;
    cin >> T;  // Read number of test cases

    while (T--) {
        int A, B, C, D;
        cin >> A >> B >> C >> D;  // Read angles of the quadrilateral

        // Check if the sum of opposite angles equals 180 degrees
        if (A + C == 180 && B + D == 180) {
            cout << "YES" << endl;
        } else {
            cout << "NO" << endl;
        }
    }

    return 0;
}
